package factorysingleton;

public class BankTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		SavingsAccount sa = new SavingsAccount();
		sa.deposit(7000);
		sa.withdraw(2000);
		
		CurrentAccount ca = new CurrentAccount();
		ca.deposit(7000);
		ca.withdraw(2000);
	}

}
