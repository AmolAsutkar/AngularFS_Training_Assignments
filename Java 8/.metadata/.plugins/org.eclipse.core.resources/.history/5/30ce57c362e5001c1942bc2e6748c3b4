class LoginService{
	private String LoggedINUser="";
	
	public void loginUser(String username,String password) {
		String hash=hashPassword(password);
		if(userExistinDB(username,hash))
			LoggedINUser=username;
	}
	
	public void logout() {
		LoggedINUser="";
	}
	
}
